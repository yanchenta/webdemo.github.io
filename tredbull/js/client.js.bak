	});
});
function selectFriends ()
{
	if (friends == ''){
        friends = $(this).find(".fselect").next().attr('data') + '^' + $(this).find(".fselect").next().html();
        if(selectFriend.length<5) {
        	selectFriend.push($(this).find(".fselect").next().attr('rel'));
        	
        	texts += '@' + $(this).find(".fselect").next().html();
        }
	}else{
        friends += '|' + $(this).find(".fselect").next().attr('data') + '^' + $(this).find(".fselect").next().html();
        
        if(selectFriend.length<5) {
        	
        	selectFriend.push($(this).find(".fselect").next().attr('rel'));
        
        	texts += '@' + $(this).find(".fselect").next().html();
        }
		
	}
}
function showPageNumList(swa,page) {
	var data = friendJsonData;
	if (data == null) return false;
	if (swa == 1) {//L
		if (page > 1) {
			var showPage = parseInt($('.pageNumB').html());
			if(showPage>1) showPage -= 1;
			
			var start = page - 1;
			pageNow = showPage;
			var end = 7 + start;
			if(end > pageCnt) end = pageCnt;
			//start=end-start;
			showPageList(showPage);
		}
	} else if (swa == 2) {//R
		if (page <= pageCnt) {
			var showPage = parseInt($('.pageNumB').html());
			if(showPage<pageCnt) showPage += 1;
			
			var start = page + 1;
			pageNow = showPage;
			var end = 7 + start;
			if(end > pageCnt) end = pageCnt;
			showPageList(showPage);
		}
	} else {//第1页
		var start = 1;
		pageNow = start;
		var end = 7;
		if(end > pageCnt) end = pageCnt;
		showPageList(start);
	}
	$('.pageNum').html('');
	var pageStr = '<table><tbody><tr>';
	for(var i=start; i<=end; i++) {
		if (i==pageNow) {
			pageStr += '<td valign="middle" align="center"><a href="javascript:;" onclick="selectPage('+i+')"><p class="num num' + i + ' pageNumB">' + i + '</p></a></td>';
		} else {
			pageStr += '<td valign="middle" align="center"><a href="javascript:;" onclick="selectPage('+i+')"><p class="num num' + i + ' pageNumR">' + i + '</p></a></td>';
		}
	}
	pageStr += '</tr></tbody></table>';
	$('.pageNum').html(pageStr);
}
function selectPage(page)
{
	//pageNow = page;
	showPageList(page);
	$('.pageNum').find('.num').removeClass('pageNumB').addClass('pageNumR');
	$('.pageNum').find('.num'+page).removeClass('pageNumR').addClass('pageNumB');
	$(".f_item").each(function () {
        $(this).click(function () {
            //alert($(this).find(".fselect").attr('class'));
            
            if (friends == '')
                friends = $(this).find(".fselect").next().attr('data') + '^' + $(this).find(".fselect").next().html();
            else
                friends += '|' + $(this).find(".fselect").next().attr('data') + '^' + $(this).find(".fselect").next().html();
            $(this).find(".fselect").toggleClass("fselected");
        });
    });
}
function showPageList(page) {
	var data = friendJsonData;
	if (data == null) return false;
	var recordCnt = data.length;
	var start = (page - 1) * 4;
	var end = start + 4;
	if (end > recordCnt) {
		end = recordCnt;
	} 
	$('.f_list').html('');
	
	for(var i=start; i<end; i++) {
		//alert(jQuery.inArray(i,selectFriend));return;
		if(jQuery.inArray(i,selectFriend)!=-1)
			{
				var chars=' fselected';
			}else{
				var chars='';
			}
		var str = ' <section class="f_item f1">' +
		'<section class="photo">' +
		'    <img src="' + data[i].head + '" alt="" width="100%" height="100%"/>' +
		'</section>' +
		'<section class="fselect'+chars+'"></section>' +
		'<section class="fname" data="' + data[i].uid + '" rel="'+i+'">' + data[i].name + '</section>' +
		'</section>';
		$('.f_list').append(str);
	}
}

function qqshare(txt) {
    $.ajaxSettings.async = false;
    var url = interfaceUri + 'ad=redbull&tag=share&k=redbull_root_qq&content=' + encodeURI(txt) + '&log=';
    $.get(url, {}, function (data) {
        if(data.code!='1')
        	{
        		alert("分享失败");
        	}
        return false;
    });
}

function netEasyLogin(uid, name, photo, plant) {
    var url = netEasyInterfaceUri + 'waplogin';
    photo = photo == '' ? 'http://test.mhttt.com/RedBull/webhtml/img/f_userP.png' : photo;
    var postData = 'nickname=' + encodeURI(name) + '&name=' + encodeURI(name) + '&openid=' + uid + '&photo=' + encodeURI(photo) + '&source=adwo&auth=' + plant;
    $.ajax({
        type: "get",
        async: false,
        url: url,
        data: postData,
        dataType: "jsonp",
        jsonp: "callback",
        jsonpCallback: "jsonpcallback",
        success: function (data) {
            if (data.retCode == 200) {
                openid = data.retData;
               $.cookie('openid',openid);
               
            }
            return false;
        }
    });
}

